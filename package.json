		"wisp-server-node": "^1.1.4"
	},
	"type": "module",
	"dependencies": {
		"@mercuryworkshop/bare-mux": "2.1.1",
		"@webreflection/idb-map": "^0.3.1",
		"devtools-detector": "^2.0.22",
		"dom-serializer": "^2.0.0",
		"domhandler": "^5.0.3",
		"domutils": "^3.1.0",
